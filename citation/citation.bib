	keywords = {Animals,Base Sequence,Blotting, Northern,Enzyme Induction,Gene Expression Profiling,Glucocorticoids,Hippocampus,Immediate-Early Proteins,In Situ Hybridization,Male,Maze Learning,Memory,Molecular Sequence Data,Nuclear Proteins,Polymerase Chain Reaction,Protein-Serine-Threonine Kinases,RNA, Messenger,Rats,Rats, Sprague-Dawley,Space Perception,Time Factors,Transfection,biosynthesis,drug effects,genetics,metabolism,pharmacology,physiology},
