	keywords = {Adult,Cohort Studies,Cross-Sectional Studies,Denmark,Family,Female,Genetic Markers,Genetic Predisposition to Disease,Humans,Infection,Male,Maternal Exposure,Pregnancy,Pregnancy Complications, Infectious,Prenatal Exposure Delayed Effects,Prevalence,Pyelonephritis,Risk Factors,Schizophrenia,epidemiology,etiology,genetics,psychology,statistics {\&} numerical data},
	language = {eng},
	month = {sep},
	number = {9},
	pages = {1025--1030},
	pmid = {19487391},
	title = {{Evidence for an interaction between familial liability and prenatal exposure to infection in the causation of schizophrenia.}},
