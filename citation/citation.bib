	keywords = {Animals,Bipolar Disorder,Brain,Calcium,Calcium Signaling,Humans,Neurogenesis,Neuronal Plasticity,Neurons,Schizophrenia,Transcription, Genetic,genetics,metabolism,pathology,physiopathology},
	language = {eng},
