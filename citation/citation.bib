	journal = {Nat Meth},
	month = {dec},
	number = {12},
	pages = {1185--1191},
	publisher = {Nature Publishing Group, a division of Macmillan Publishers Limited. All Rights Reserved.},
	title = {{Systematic evaluation of spliced alignment programs for RNA-seq data}},
	url = {http://dx.doi.org/10.1038/nmeth.2722 10.1038/nmeth.2722 http://www.nature.com/nmeth/journal/v10/n12/abs/nmeth.2722.html{\#}supplementary-information},
	volume = {10},
